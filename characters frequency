 'use strict';

 function charFrequency(str) {

    let count = {};



     for(let i=0; i<str.length;i++){
         let char = str[i];
         if(count[char]){
             count[char]++
         }else{
             count[char]=1
         }
     }
     return count;
 }


let str = "programming";
 console.log(charFrequency(str));
 // output: { p:1, r:2, o:1, g:2, a:1, m:2, i:1, n:1 }
