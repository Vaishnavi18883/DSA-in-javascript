'use strict';

function moveZeros(arr) {
    // Write your code here
    let j= 0
    for(let i=1;i<arr.length;i++){
        
            if(arr[i]!==0){
                let temp = arr[i];
                arr[i]= arr[j]
                arr[j]= temp
                j++

            }
        }
    

    return arr;
}

let arr = [0, 1, 0, 3, 12];
console.log(moveZeros(arr));
